{{#with user}}
<div class="container container-marg-bot">

    <p>Welcome {{username}}, here you can look for your previous researches and more info:</p>
    {{/with}}

    <div class="row">
        <div class="col-md-4 researches-cont">

            <ul>
                {{#each user.search_id}}
                <li>
                    <a href="/auth/lookups/{{this._id}}" class="searches-btns">{{this.place}}s in {{this.zone}}</a>
                </li>
                {{else}}
                <p class="non-shadow">You got no searches yet <a href="profile">Make one!</a></p>
                {{/each}}
            </ul>

        </div>

